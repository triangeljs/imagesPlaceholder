<!DOCTYPE html>
<html lang="zh">
<head>
  <meta charset="UTF-8">
  <title>图片上传</title>
  <link rel="stylesheet" href="/stylesheets/webuploader.css">
  <script type="text/javascript" src="http://www.china.com.cn/lib/jquery-1.11.0.min.js"></script>
  <script type="text/javascript" src="/javascripts/webuploader.js"></script>
</head>
<body>
  <!--dom结构部分-->
  <div id="uploader-demo">
    <!--用来存放item-->
    <div id="filePicker">选择图片</div>
    <div id="uploadBtn" class="uploadBtn">上传</div>
    <% if (picTypeData.length) { %>
    <div class="selectBox">
      <select name="select" id="select">
        <option value="0">选择要上传的类型</option>
        <% picTypeData.forEach(function(picTypeList){ %>
        <option value="<%= picTypeList.directory %>"><%= picTypeList.title %></option>
        <% }) %>
      </select>
    </div>
    <% } %>
    <div id="fileList" class="uploader-list"></div>
  </div>
  <script type="text/javascript" src="/javascripts/picUpload.js"></script>
</body>
</html>